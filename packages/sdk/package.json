{
  "name": "@solweave/sdk",
  "version": "0.0.2",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./.dist/cjs/index.js",
  "module": "./.dist/esm/index.js",
  "files": [
    ".dist/"
  ],
  "scripts": {
    "watch": "npm run build",
    "dev": "ENVIRONMENT_NAME=dev npm-watch watch",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "start": "node .dist/index.js"
  },
  "watch": {
    "watch": {
      "patterns": [
        "src"
      ],
      "quiet": false,
      "legacyWatch": true,
      "extensions": "ts, js, tsx"
    }
  },
  "dependencies": {
    "@project-serum/anchor": "^0.18.2",
    "@solana/wallet-adapter-react": "^0.13.1",
    "@solana/web3.js": "^1.30.2",
    "@solweave/cost-calculator": "^0.0.2",
    "@types/axios": "^0.14.0",
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "npm-watch": "^0.5.0",
    "typescript": "^4.2.4"
  }
}
